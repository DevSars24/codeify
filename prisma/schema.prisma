generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
}

model ContestAttempt {
  id        String   @id @default(cuid())
  userId    String
  topic     String
  accuracy  Int
  createdAt DateTime @default(now())
  correct   Int
  language  String
  total     Int

  @@index([userId])
}
